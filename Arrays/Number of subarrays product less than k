class Solution {
    public int numSubarrayProductLessThanK(int[] nums, int k) {
        int count =  0;
       int product = 1;
       int i = 0;

       for(int j = 0; j < nums.length; j++){
        product = product * nums[j];
       
        while(product >= k && i <= j){
        product = product / nums[i];
        i++;
        }
        count = count + (j-i+1);
        }
       return count;
    }
}
